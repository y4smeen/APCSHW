import java.util.*;

public class ArrayList {
    public static void main(String[] args) {
			
		ArrayList<Integer> ai = new ArrayList<Integer>();

		Random r = new Random();
		for (int i = 0; i < 20; i++) {
				ai.add(r.nextInt(5));
		}
		
		ArrayList<Integer aj = new ArrayList<Integer>();
		
		for (int k = 0; aj.size() < ai.size(); k++) {
		    int current = 0;
		    if (ai.get(k) == current) {
			aj.add(ai.get(k));
		    }
		    if (ai.indexOf(current) == -1) {
			current++;
		    }
		}

		    
    }
}
